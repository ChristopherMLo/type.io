<div id="body">
    <div id="header">
        Room #<span id="roomNumber">1</span>
        Hello, <span id="player">Guest</span>
    </div>
    <div id="gameWrapper">
        <div id="preGameWindow">
            <div class="container">
                <div class="row panel">
                    <div class="col-md">
                        <div class="player1 playerbox">EMPTY</div>
                    </div>
                    <div class="col-md">
                        <div class="player2 playerbox">EMPTY</div>
                    </div>
                    <div class="col-md">
                        <div class="player3 playerbox">EMPTY</div>
                    </div>
                    <div class="col-md">
                        <div class="player4 playerbox">EMPTY</div>
                    </div>
                </div>
                <div class="row justify-content-center">
                    <button onclick="startGame()" class="btn btn-primary">Start Game</button>
                </div>
            </div>
        </div>

        <div id="gameWindow">
            <div class="container">
                <div class="row justify-content-center panel">
                    <ul id="goal">
                    </ul>
                </div>
                <div class="row justify-content-center panel">
                    <div id="typed" class="col"><br></div>
                </div>
                <div class="row justify-content-center panel">
                    <div id="message" class="col"><br></div>
                </div>
                <div class="row justify-content-center panel">
                    <input id="input" type="text" size=1 onkeyup="keyTyped(event)">
                </div>
                <div class="row panel">
                    <div class="col-md">
                        <div class="player1 playerbox">EMPTY</div>
                    </div>
                    <div class="col-md">
                        <div class="player2 playerbox">EMPTY</div>
                    </div>
                    <div class="col-md">
                        <div class="player3 playerbox">EMPTY</div>
                    </div>
                    <div class="col-md">
                        <div class="player4 playerbox">EMPTY</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="chatWrapper">
        <div id="chatBody">
            <ul id="messages"></ul>
        </div>
        <input id="chatInput" autocomplete="off"/>
    </div>
</div>
